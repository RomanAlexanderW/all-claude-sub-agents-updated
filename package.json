{
  "name": "all-claude-sub-agents-updated",
  "version": "3.0.0",
  "description": "610+ specialized AI agents for Claude Flow v3 with SONA neural architecture, 17 hooks + 12 background workers, and hybrid memory system",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "claude",
    "claude-flow",
    "claude-flow-v3",
    "ai-agents",
    "agentdb",
    "sona",
    "reasoning-bank",
    "hive-mind",
    "swarm-intelligence",
    "mcp-tools",
    "orchestration",
    "multi-agent",
    "memory-system",
    "vector-search",
    "neural-architecture",
    "agent-booster"
  ],
  "author": "Claude Flow Community + RomanAlexanderW",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RomanAlexanderW/all-claude-sub-agents-updated"
  },
  "dependencies": {
    "claude-flow": "^3.0.0-alpha.185",
    "agentdb": "^2.0.0",
    "better-sqlite3": "^11.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.0.1",
    "@types/node": "^22.0.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "init": "npx claude-flow@v3alpha init --force",
    "init:wizard": "npx claude-flow@v3alpha init --wizard",
    "swarm": "npx claude-flow@v3alpha swarm",
    "swarm:create": "npx claude-flow@v3alpha swarm create",
    "swarm:status": "npx claude-flow@v3alpha swarm status",
    "hive-mind": "npx claude-flow@v3alpha hive-mind wizard",
    "hive:spawn": "npx claude-flow@v3alpha hive-mind spawn",
    "hive:consensus": "npx claude-flow@v3alpha hive-mind consensus",
    "agent": "npx claude-flow@v3alpha agent",
    "agent:spawn": "npx claude-flow@v3alpha agent spawn",
    "agent:list": "npx claude-flow@v3alpha agent list",
    "agent:status": "npx claude-flow@v3alpha agent status",
    "agent:metrics": "npx claude-flow@v3alpha agent metrics",
    "memory": "npx claude-flow@v3alpha memory",
    "memory:status": "npx claude-flow@v3alpha memory status",
    "memory:store": "npx claude-flow@v3alpha memory store",
    "memory:search": "npx claude-flow@v3alpha memory search",
    "memory:vector": "npx claude-flow@v3alpha memory vector-search",
    "memory:query": "npx claude-flow@v3alpha memory query",
    "neural": "npx claude-flow@v3alpha neural",
    "neural:train": "npx claude-flow@v3alpha neural train",
    "neural:patterns": "npx claude-flow@v3alpha neural patterns",
    "neural:status": "npx claude-flow@v3alpha neural status",
    "hooks": "npx claude-flow@v3alpha hooks",
    "hooks:list": "npx claude-flow@v3alpha hooks list",
    "hooks:run": "npx claude-flow@v3alpha hooks run",
    "daemon": "npx claude-flow@v3alpha daemon",
    "daemon:start": "npx claude-flow@v3alpha daemon start",
    "daemon:trigger": "npx claude-flow@v3alpha daemon trigger",
    "mcp:start": "npx claude-flow@v3alpha mcp start",
    "mcp:add": "claude mcp add claude-flow -- npx claude-flow@v3alpha mcp start",
    "mcp:status": "npx claude-flow@v3alpha mcp status",
    "migrate": "npx claude-flow@v3alpha migrate run",
    "migrate:status": "npx claude-flow@v3alpha migrate status",
    "migrate:verify": "npx claude-flow@v3alpha migrate verify",
    "migrate:rollback": "npx claude-flow@v3alpha migrate rollback",
    "validate": "node scripts/validate-agents.js",
    "convert": "python convert_agents.py",
    "test": "echo \"No tests configured\"",
    "docs": "echo \"See ./docs directory for documentation\""
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "claude-flow": {
    "version": "3.0.0",
    "agentDirectory": ".claude/agents",
    "agentCount": 610,
    "memoryBackend": "hybrid",
    "neuralArchitecture": "SONA",
    "hooksCount": 17,
    "workersCount": 12,
    "modelRouting": {
      "tier1": "agent-booster",
      "tier2": "haiku",
      "tier3": "sonnet-opus"
    }
  }
}
