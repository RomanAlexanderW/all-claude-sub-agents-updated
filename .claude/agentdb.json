{
  "version": "2.0.0",
  "backend": "hybrid",
  "backends": {
    "sqlite": {
      "enabled": true,
      "database": ".swarm/memory.db"
    },
    "agentdb": {
      "enabled": true,
      "database": ".swarm/agentdb.db"
    }
  },
  "vectorSearch": {
    "enabled": true,
    "algorithm": "HNSW",
    "indexing": "O(log n)",
    "dimensions": 1024,
    "speedImprovement": "150x-12500x faster",
    "hyperbolicEmbeddings": {
      "enabled": true,
      "model": "Poincar√© ball",
      "useCase": "hierarchical relationships"
    }
  },
  "embeddingProviders": {
    "primary": "agentic-flow-onnx",
    "fallback": ["openai", "transformers-js", "mock"],
    "latency": {
      "agentic-flow-onnx": "3ms",
      "openai": "100-200ms",
      "transformers-js": "50-100ms"
    }
  },
  "sona": {
    "enabled": true,
    "adaptationTime": "<0.05ms",
    "ewcPlusPlus": {
      "enabled": true,
      "description": "Elastic Weight Consolidation++ prevents catastrophic forgetting"
    },
    "learningCycle": ["retrieve", "judge", "distill", "consolidate", "route"]
  },
  "reinforcementLearning": {
    "algorithms": [
      "Q-Learning",
      "PPO",
      "MCTS",
      "Decision-Transformer",
      "DQN",
      "A2C",
      "SAC",
      "TD3",
      "Rainbow"
    ]
  },
  "ruvector": {
    "enabled": true,
    "sqlFunctions": 77,
    "attentionMechanisms": 39,
    "gnnLayerTypes": 15
  },
  "features": {
    "reflexionMemory": true,
    "skillLibrary": true,
    "causalReasoning": true,
    "retrieveJudgeDistill": true,
    "quantization": {
      "binary": "32x reduction",
      "scalar": "4x reduction",
      "product": "8-16x reduction"
    },
    "memoryReduction": "50-75%"
  },
  "performance": {
    "vectorSearch": "150x-12500x faster",
    "batchOperations": "125x faster",
    "largeQueries": "164x faster",
    "queryLatency": "<1ms",
    "targets": {
      "cliColdStart": "<500ms",
      "mcpServerInit": "<400ms",
      "agentSpawn": "<200ms",
      "vectorSearch": "<1ms",
      "memoryWrite": "<5ms"
    }
  }
}
